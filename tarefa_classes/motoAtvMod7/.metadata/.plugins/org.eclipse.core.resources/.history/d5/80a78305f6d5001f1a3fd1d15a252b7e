package br.com.mont;

public class UsarMoto {

		public static void main(String [] args) {
			moto c1 = new moto();//criando um objeto
			
			//chamando os atributos
			c1.piloto = "Lorran";
			c1.ignicion = false;
			c1.vel = 0;
			c1.marcha = 0;
			
			//chamando os m√©todos
			c1.ligar();
			
			
			
			
		}
}
